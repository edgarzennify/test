    private static testMethod void it_should_insert_the_attachment() {
        EB_Appraisal_Order__c testAppraisalOrder = new EB_Appraisal_Order__c();
        insert testAppraisalOrder;
        Attachment attach = new Attachment();
        attach.Name = 'AppraisalOrderSnapshot.pdf';
        attach.IsPrivate = false;
        attach.ParentId = testAppraisalOrder.Id;
        attach.Body = Blob.valueOf('Some Text');

        sut.insertAttachment(attach);
        
        Attachment returnedAttachment = [SELECT Id, Name FROM Attachment WHERE ParentId =: testAppraisalOrder.Id];

        System.assertEquals('AppraisalOrderSnapshot.pdf', returnedAttachment.Name);
        System.assertNotEquals(null, returnedAttachment.Id);
    }


